<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="trackBoxHolder" styleClass="trackBoxBaseStyle" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="syncgod.track.TrackBox">
    <VBox maxHeight="40.0" maxWidth="100.0" minHeight="40.0" minWidth="100.0" prefHeight="40.0" prefWidth="100.0" styleClass="trackBoxLabelHolder">
        <Label fx:id="trackBoxLabel" alignment="CENTER" maxHeight="40.0" maxWidth="100.0" minHeight="20.0" minWidth="100.0" styleClass="trackBoxLabel" text="Track" />
        <Label fx:id="trackBoxFrameLabel" alignment="CENTER" maxHeight="20.0" maxWidth="100.0" minHeight="20.0" minWidth="100.0" prefHeight="20.0" prefWidth="100.0" styleClass="trackBoxFrameLabel" visible="false" />
    </VBox>
    <Region fx:id="trackBoxOffset" styleClass="trackBoxOffset" />
    <AnchorPane fx:id="trackBoxPane" maxHeight="40.0" minHeight="40.0" onMouseEntered="#onMouseEntered" onMouseDragged="#onMouseDragged" onMouseExited="#onMouseExited" onMouseMoved="#onMouseMoved" onMousePressed="#onMousePressed" onMouseReleased="#onMouseReleased" styleClass="trackBoxPane" />
</HBox>
