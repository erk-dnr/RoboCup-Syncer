<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>

<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="800.0" prefWidth="1600.0"
      styleClass="mainWindow" xmlns="http://javafx.com/javafx/8.0.172-ea"
      fx:controller="syncgod.scene.Scene">
    <fx:include source="menu/Menu.fxml"/>
    <SplitPane dividerPositions="0.8" VBox.vgrow="ALWAYS">
        <SplitPane dividerPositions="0.8" orientation="VERTICAL" prefHeight="200.0"
                   prefWidth="160.0">
            <fx:include source="VideoPane.fxml"/>
            <fx:include source="TrackPane.fxml"/>
        </SplitPane>
        <fx:include maxHeight="1.7976931348623157E308" source="LogPane.fxml"/>
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </VBox.margin>
    </SplitPane>
    <stylesheets>
        <URL value="@../styles/Styles.css"/>
    </stylesheets>
</VBox>
